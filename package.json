{
  "name": "legend-builder",
  "version": "1.0.0-alpha.15",
  "description": "An advanced League Of Legends champion builder",
  "bugs": {
    "url": "https://github.com/SteveVanOpstal/LegendBuilder/issues"
  },
  "license": "GPL-3.0",
  "author": {
    "name": "Steve Van Opstal",
    "url": "https://github.com/SteveVanOpstal"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SteveVanOpstal/LegendBuilder.git"
  },
  "homepage": "http://legendbuilder.io/",
  "scripts": {
    "ng": "ng",
    "serve": "ng serve",
    "start": "node config/build/start.js",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "coverage": "npm run coveralls < ./coverage/lcov/lcov.info",
    "prebuild": "mkdir dist || true",
    "build": "npm run build:client && npm run build:server",
    "build:client": "ng build",
    "build:client:prod": "ng build --prod",
    "build:server": "npm run webpack -- --config config/build/webpack.server.js --progress --env.dev",
    "build:server:prod": "npm run webpack -- --config config/build/webpack.server.js --progress --bail",
    "build:server:test": "npm run webpack -- --config config/build/webpack.server.test.js --progress",
    "static-server": "npm run build:server && node dist/server/static-server.js --api=./secure/api.key --cert=./secure/cert.crt --key=./secure/cert.key",
    "static-server:prod": "npm run build:server:prod && node dist/server/static-server.js --api=./secure/api.key --cert=./secure/cert.crt --key=./secure/cert.key",
    "match-server": "npm run build:server && node dist/server/match-server.js --api=./secure/api.key --cert=./secure/cert.crt --key=./secure/cert.key",
    "match-server:prod": "npm run build:server:prod && node dist/server/match-server.js --api=./secure/api.key --cert=./secure/cert.crt --key=./secure/cert.key",
    "format": "npm run clang-format -- --glob=\"@(src|config)/**/!(browser-providers.conf).@(ts|js)\" -i",
    "test": "npm run test:server && npm run test:client",
    "test:client": "ng test",
    "pretest:server": "npm run build:server:test",
    "test:server": "node jasmine.js",
    "ci": "node config/build/ci.js",
    "sauce": "node config/build/sauce.js",
    "sauce-connect": "node config/build/sauce-connect.js",
    "sauce:open": "npm run sauce",
    "sauce:close": "npm run sauce -- --close",
    "s3:upload": "node config/build/s3.js --upload",
    "s3:delete": "node config/build/s3.js --delete",
    "github-release": "npm run standard-version -- -p alpha -t",
    "postgithub-release": "git push --follow-tags origin master && npm run conventional-github-releaser",
    "reddit-release": "python config/build/reddit/release.py",
    "release": "npm run github-release && npm run reddit-release",
    "coveralls": "./node_modules/.bin/coveralls",
    "webpack": "./node_modules/.bin/webpack",
    "clang-format": "./node_modules/.bin/clang-format",
    "protractor": "./node_modules/.bin/protractor",
    "karma": "./node_modules/.bin/karma",
    "standard-version": "./node_modules/.bin/standard-version",
    "csscomb": "./node_modules/.bin/csscomb -c ./config/csscomb.json",
    "conventional-github-releaser": "./node_modules/.bin/conventional-github-releaser -p angular"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^4.2.4",
    "@angular/compiler": "^4.2.4",
    "@angular/core": "^4.2.4",
    "@angular/http": "^4.2.4",
    "@angular/platform-browser": "^4.2.4",
    "@angular/platform-browser-dynamic": "^4.2.4",
    "@angular/router": "^4.2.4",
    "async": "^2.5.0",
    "chalk": "^2.1.0",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.1",
    "d3-array": "^1.2.0",
    "d3-axis": "^1.0.8",
    "d3-color": "^1.0.3",
    "d3-format": "^1.2.0",
    "d3-interpolate": "^1.1.5",
    "d3-path": "^1.0.5",
    "d3-scale": "^1.0.6",
    "d3-selection": "^1.1.0",
    "d3-shape": "^1.2.0",
    "dateformat": "^2.0.0",
    "lru-cache": "^4.1.1",
    "minimist": "^1.2.0",
    "ng-pipes": "^0.1.2",
    "rxjs": "^5.4.2",
    "webfontloader": "^1.6.28",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "^1.4.1",
    "@angular/compiler-cli": "^4.2.4",
    "@angular/language-service": "^4.2.4",
    "@types/async": "2.0.40",
    "@types/d3-array": "1.2.0",
    "@types/d3-axis": "1.0.8",
    "@types/d3-color": "1.0.5",
    "@types/d3-format": "1.2.0",
    "@types/d3-interpolate": "1.1.6",
    "@types/d3-path": "1.0.6",
    "@types/d3-scale": "1.0.10",
    "@types/d3-selection": "1.1.0",
    "@types/d3-shape": "1.2.0",
    "@types/jasmine": "~2.5.53",
    "@types/jasmine-matchers": "^0.2.30",
    "@types/jasminewd2": "~2.0.2",
    "@types/minimist": "1.2.0",
    "@types/node": "^8.0.26",
    "@types/webfontloader": "1.6.28",
    "@types/webpack": "^3.0.10",
    "add-matchers": "^0.5.0",
    "clang-format": "^1.0.53",
    "codelyzer": "~3.1.1",
    "conventional-github-releaser": "^1.1.12",
    "coveralls": "^2.13.1",
    "csscomb": "^4.2.0",
    "glob": "^7.1.2",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "raw-loader": "^0.5.1",
    "s3": "^4.4.0",
    "sauce-connect-launcher": "^1.2.2",
    "standard-version": "^4.2.0",
    "svgo-loader": "^1.2.1",
    "ts-node": "~3.2.0",
    "tslint": "~5.3.2",
    "typescript": "~2.3.3"
  }
}

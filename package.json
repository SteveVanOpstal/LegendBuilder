{
  "name": "legend-builder",
  "version": "1.0.0-alpha.13",
  "description": "An advanced League Of Legends champion builder",
  "bugs": {
    "url": "https://github.com/SteveVanOpstal/LegendBuilder/issues"
  },
  "license": "GPL-3.0",
  "author": {
    "name": "Steve Van Opstal",
    "url": "https://github.com/SteveVanOpstal"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SteveVanOpstal/LegendBuilder.git"
  },
  "homepage": "http://legendbuilder.io/",
  "scripts": {
    "preclean": "npm run rimraf coverage build node_modules || true",
    "clean": "npm cache clean",
    "mkdir:build": "mkdir build || true",
    "mkdir:build:profile": "mkdir build\\profile || true",
    "coverage": "npm run coveralls < ./coverage/lcov/lcov.info",
    "build": "npm run build:client && npm run build:server",
    "build:client": "npm run build:client:prod",
    "build:client:dev": "npm run webpack:client -- --env.dev",
    "build:client:prod": "npm run webpack:client -- --bail",
    "build:client:test": "npm run webpack:client:test",
    "prebuild:client:profile": "npm run mkdir:build && npm run mkdir:build:profile",
    "build:client:profile": "npm run webpack --silent -- --config config/build/webpack.client.js --env.profile --profile --json > build/profile/stats.json",
    "build:server": "npm run build:server:prod",
    "build:server:dev": "npm run webpack:server -- --env.dev",
    "build:server:prod": "npm run webpack:server -- --bail",
    "build:server:test": "npm run webpack:server:test",
    "prewebpack:client": "npm run lint:client",
    "webpack:client": "npm run webpack -- --config config/build/webpack.client.js --progress",
    "prewebpack:server": "npm run lint:server",
    "webpack:server": "npm run webpack -- --config config/build/webpack.server.js --progress",
    "prewebpack:client:test": "npm run lint:client:test",
    "webpack:client:test": "npm run webpack -- --config config/build/webpack.client.test.js --progress",
    "prewebpack:server:test": "npm run lint:server:test",
    "webpack:server:test": "npm run webpack -- --config config/build/webpack.server.test.js --progress",
    "start": "node config/build/start.js",
    "server": "npm run http-server",
    "http-server": "npm run http-server:dev",
    "http-server:dev": "npm run webpack-dev-server -- --env.dev --env.server",
    "http-server:prod": "npm run webpack-dev-server -- --bail",
    "static-server": "npm run static-server:dev",
    "static-server:dev": "npm run build:server:dev && node build/dist/server/static-server.js",
    "static-server:prod": "npm run build:server:prod && node build/dist/server/static-server.js",
    "match-server": "npm run match-server:dev -- --api=./secure/api.key --cert=./secure/cert.pem --key=./secure/key.pem",
    "match-server:dev": "npm run build:server:dev && node build/dist/server/match-server.js",
    "match-server:prod": "npm run build:server:prod && node build/dist/server/match-server.js",
    "format": "npm run clang-format -- --glob=\"@(src|config)/**/!(browser-providers.conf).@(ts|js)\" -i",
    "format:client": "npm run clang-format -- --glob=\"src/client/**/*.@(ts|js)\" -i",
    "format:server": "npm run clang-format -- --glob=\"src/server/**/*.@(ts|js)\" -i",
    "format:css": "npm run csscomb -- src",
    "lint": "npm run tslint -- src/**/*.ts",
    "lint:client": "npm run tslint -- src/client/**/*.ts -e **/*.spec.ts -e **/*.e2e.ts -e **/testing/**",
    "lint:server": "npm run tslint -- src/server/**/*.ts -e **/*.spec.ts -e **/*.e2e.ts -e **/testing/**",
    "lint:client:test": "npm run tslint -- src/client/**/*.spec.ts src/client/**/testing/**/*.ts",
    "lint:server:test": "npm run tslint -- src/server/**/*.spec.ts src/server/**/testing/**/*.ts",
    "lint:test": "npm run tslint -- src/**/*.spec.ts src/**/testing/**/*.ts",
    "lint:e2e": "npm run lint src/**/*.e2e.ts",
    "webdriver:update": "npm run webdriver-manager -- update",
    "pree2e": "npm run lint:e2e && npm run webdriver:update -- --standalone",
    "e2e": "npm run protractor -- config/protractor.conf.js",
    "e2e:live": "npm run protractor -- config/protractor.conf.js --elementExplorer",
    "test": "npm run test:server && npm run test:client",
    "pretest:client": "npm run lint:client:test",
    "test:client": "npm run karma -- start config/karma.conf.js",
    "pretest:server": "npm run build:server:test",
    "test:server": "node config/jasmine.js",
    "ci": "node config/build/ci.js",
    "sauce": "node config/build/sauce.js",
    "sauce-connect": "node config/build/sauce-connect.js",
    "sauce:open": "npm run sauce",
    "sauce:close": "npm run sauce -- --close",
    "s3:upload": "node config/build/s3.js --upload",
    "s3:delete": "node config/build/s3.js --delete",
    "github-release": "npm run standard-version -- -p alpha -t",
    "postgithub-release": "git push --follow-tags origin master && npm run conventional-github-releaser",
    "reddit-release": "python config/build/reddit/release.py",
    "release": "npm run github-release && npm run reddit-release",
    "rimraf": "./node_modules/.bin/rimraf",
    "coveralls": "./node_modules/.bin/coveralls",
    "webpack": "./node_modules/.bin/webpack",
    "webpack-dev-server": "./node_modules/.bin/webpack-dev-server --inline --progress --compress --config config/build/webpack.client.js --watch --content-base src/ --https --cert=./secure/cert.pem --key=./secure/key.pem",
    "clang-format": "./node_modules/.bin/clang-format",
    "tslint": "./node_modules/.bin/tslint --config ./config/tslint.json",
    "webdriver-manager": "./node_modules/.bin/webdriver-manager",
    "protractor": "./node_modules/.bin/protractor",
    "karma": "./node_modules/.bin/karma",
    "standard-version": "./node_modules/.bin/standard-version",
    "csscomb": "./node_modules/.bin/csscomb -c ./config/csscomb.json",
    "conventional-github-releaser": "./node_modules/.bin/conventional-github-releaser -p angular"
  },
  "dependencies": {
    "@angular/animations": "4.0.0-rc.3",
    "@angular/common": "4.0.0-rc.3",
    "@angular/compiler": "4.0.0-rc.3",
    "@angular/core": "4.0.0-rc.3",
    "@angular/http": "4.0.0-rc.3",
    "@angular/platform-browser": "4.0.0-rc.3",
    "@angular/platform-browser-dynamic": "4.0.0-rc.3",
    "@angular/router": "4.0.0-rc.3",
    "@types/async": "2.0.38",
    "@types/core-js": "0.9.35",
    "@types/d3-array": "1.0.7",
    "@types/d3-axis": "1.0.6",
    "@types/d3-color": "1.0.4",
    "@types/d3-format": "1.0.7",
    "@types/d3-interpolate": "1.1.5",
    "@types/d3-path": "1.0.5",
    "@types/d3-scale": "1.0.6",
    "@types/d3-selection": "1.0.9",
    "@types/d3-shape": "1.0.7",
    "@types/minimist": "1.2.0",
    "@types/node": "7.0.4",
    "@types/webfontloader": "1.6.28",
    "async": "2.1.4",
    "chalk": "1.1.3",
    "core-js": "2.4.1",
    "d3-array": "1.0.2",
    "d3-axis": "1.0.4",
    "d3-color": "1.0.2",
    "d3-format": "1.0.2",
    "d3-interpolate": "1.1.3",
    "d3-path": "1.0.3",
    "d3-scale": "1.0.4",
    "d3-selection": "1.0.3",
    "d3-shape": "1.0.4",
    "dateformat": "2.0.0",
    "lru-cache": "4.0.2",
    "minimist": "1.2.0",
    "ng-pipes": "0.1.1",
    "reflect-metadata": "0.1.9",
    "rxjs": "5.1.0",
    "typescript": "2.1.6",
    "webfontloader": "1.6.27",
    "zone.js": "0.7.6"
  },
  "devDependencies": {
    "@types/jasmine": "2.5.45",
    "@types/webpack": "^2.1.0",
    "awesome-typescript-loader": "^3.0.0-beta.17",
    "clang-format": "^1.0.44",
    "clean-webpack-plugin": "^0.1.15",
    "codelyzer": "^2.0.0-beta.3",
    "conventional-github-releaser": "^1.1.3",
    "copy-webpack-plugin": "^4.0.0",
    "coveralls": "^2.11.13",
    "css-loader": "^0.26.0",
    "csscomb": "^3.1.7",
    "glob": "^7.0.3",
    "html-webpack-plugin": "^2.24.1",
    "istanbul-instrumenter-loader": "0.2.0",
    "jasmine-core": "^2.5.2",
    "jasmine-matchers-loader": "^0.1.0",
    "json-loader": "^0.5.4",
    "karma": "^1.5.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.0.0",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-sauce-launcher": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.2",
    "phantomjs-polyfill": "^0.0.2",
    "phantomjs-prebuilt": "^2.1.7",
    "protractor": "^5.1.0",
    "raw-loader": "^0.5.1",
    "resource-hints-webpack-plugin": "^0.0.1",
    "rimraf": "^2.5.2",
    "s3": "^4.4.0",
    "sauce-connect-launcher": "^1.2.0",
    "source-map-loader": "^0.1.5",
    "standard-version": "^4.0.0",
    "svgo-loader": "^1.2.1",
    "tslint": "^4.1.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.1.0-beta.10",
    "webpack-md5-hash": "^0.0.5"
  }
}
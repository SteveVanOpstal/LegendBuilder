{
  "name": "legend-builder",
  "version": "1.0.0-alpha.10",
  "description": "An advanced League Of Legends champion builder",
  "bugs": {
    "url": "https://github.com/SteveVanOpstal/LegendBuilder/issues"
  },
  "license": "GPL-3.0",
  "author": {
    "name": "Steve Van Opstal",
    "url": "https://github.com/SteveVanOpstal"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SteveVanOpstal/LegendBuilder.git"
  },
  "homepage": "http://legendbuilder.io/",
  "scripts": {
    "preclean": "./node_modules/.bin/rimraf coverage build node_modules",
    "clean": "npm cache clean",
    "clean:build:client": "./node_modules/.bin/rimraf build/dist/client",
    "clean:build:server": "./node_modules/.bin/rimraf build/dist/server",
    "clean:build:spec:client": "./node_modules/.bin/rimraf build/spec/client",
    "clean:build:spec:server": "./node_modules/.bin/rimraf build/spec/server",
    "coveralls": "./node_modules/.bin/coveralls < ./coverage/lcov/lcov.info",
    "preclean:install": "npm run clean",
    "clean:install": "npm set progress=false && npm install",
    "preclean:start": "npm run clean",
    "clean:start": "npm start",
    "build": "npm run build:client && npm run build:server",
    "build:client": "npm run build:client:prod",
    "prebuild:client:dev": "npm run clean:build:client && npm run lint:client",
    "build:client:dev": "./node_modules/.bin/webpack --config config/webpack/client/dev.js --progress --profile",
    "prebuild:client:prod": "npm run clean:build:client && npm run lint:client",
    "build:client:prod": "./node_modules/.bin/webpack --config config/webpack/client/prod.js --progress --profile --bail",
    "prebuild:client:test": "npm run clean:build:spec:client && npm run lint:client:test",
    "build:client:test": "./node_modules/.bin/webpack --config config/webpack/client/test.js --progress --profile",
    "build:server": "npm run build:server:prod",
    "prebuild:server:dev": "npm run clean:build:server && npm run lint:server",
    "build:server:dev": "./node_modules/.bin/webpack --config config/webpack/server/dev.js --progress --profile",
    "prebuild:server:prod": "npm run clean:build:server && npm run lint:server",
    "build:server:prod": "./node_modules/.bin/webpack --config config/webpack/server/prod.js --progress --profile --bail",
    "prebuild:server:test": "npm run clean:build:spec:server && npm run lint:server:test",
    "build:server:test": "./node_modules/.bin/webpack --config config/webpack/server/test.js --progress --profile",
    "webdriver:update": "./node_modules/.bin/webdriver-manager update",
    "webdriver:start": "./node_modules/.bin/webdriver-manager start",
    "start": "npm run http-server",
    "server": "npm run http-server",
    "http-server": "npm run http-server:dev",
    "http-server:dev": "./node_modules/.bin/webpack-dev-server --config config/webpack/client/dev.js --inline --progress --profile --watch --content-base src/",
    "http-server:prod": "node dist/server/http-server.js",
    "static-server": "npm run static-server:dev",
    "static-server:dev": "npm run build:server:dev && node build/dist/server/static-server.js",
    "static-server:prod": "npm run build:server:prod && node build/dist/server/static-server.js",
    "match-server": "npm run match-server:dev",
    "match-server:dev": "npm run build:server:dev && node build/dist/server/match-server.js",
    "match-server:prod": "npm run build:server:prod && node build/dist/server/match-server.js",
    "clang-format": "./node_modules/.bin/clang-format",
    "format": "npm run clang-format -- --glob=\"@(src|config)/**/!(browser-providers.conf).@(ts|js)\" -i",
    "format:client": "npm run clang-format -- --glob=\"src/client/**/*.@(ts|js)\" -i",
    "format:server": "npm run clang-format -- --glob=\"src/server/**/*.@(ts|js)\" -i",
    "lint": "./node_modules/.bin/tslint src/**/*.ts",
    "lint:client": "./node_modules/.bin/tslint src/client/**/*.ts -e **/*.spec.ts -e **/*.e2e.ts -e **/testing/**",
    "lint:server": "./node_modules/.bin/tslint src/server/**/*.ts -e **/*.spec.ts -e **/*.e2e.ts -e **/testing/**",
    "lint:client:test": "./node_modules/.bin/tslint src/client/**/*.spec.ts src/client/**/testing/**/*.ts",
    "lint:server:test": "./node_modules/.bin/tslint src/server/**/*.spec.ts src/server/**/testing/**/*.ts",
    "lint:test": "./node_modules/.bin/tslint src/**/*.spec.ts src/**/testing/**/*.ts",
    "lint:e2e": "./node_modules/.bin/tslint src/**/*.e2e.ts",
    "pree2e": "npm run lint:e2e && npm run webdriver:update -- --standalone",
    "e2e": "./node_modules/.bin/protractor config/protractor.conf.js",
    "e2e:live": "./node_modules/.bin/protractor config/protractor.conf.js --elementExplorer",
    "test": "npm run test:server && npm run test:client",
    "pretest:client": "npm run lint:client:test",
    "test:client": "./node_modules/.bin/karma start config/karma.conf.js",
    "pretest:server": "npm run build:server:test",
    "test:server": "node config/jasmine.js",
    "ci": "node config/build/ci.js",
    "s3:upload": "node config/build/s3.js",
    "reddit-release": "python src/reddit/release.py",
    "release": "./node_modules/.bin/standard-version && npm run reddit-release"
  },
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/router": "3.0.0",
    "@types/async": "2.0.32",
    "@types/auth0-js": "0.0.4",
    "@types/core-js": "^0.9.34",
    "@types/d3-axis": "1.0.5",
    "@types/d3-scale": "1.0.4",
    "@types/d3-selection": "1.0.4",
    "@types/d3-shape": "1.0.5",
    "@types/jasmine": "2.2.34",
    "@types/node": "6.0.39",
    "@types/protractor": "1.5.19",
    "@types/selenium-webdriver": "2.44.29",
    "angular2-jwt": "0.1.23",
    "async": "2.0.1",
    "auth0-js": "7.2.0",
    "chalk": "1.1.3",
    "core-js": "2.4.1",
    "d3-axis": "1.0.3",
    "d3-scale": "1.0.3",
    "d3-selection": "1.0.2",
    "d3-shape": "1.0.3",
    "dateformat": "1.0.12",
    "loader-utils": "0.2.16", /* remove when webpack/webpack#3060 is fixed */
    "lru-cache": "4.0.1",
    "reflect-metadata": "0.1.8",
    "rxjs": "5.0.0-beta.12",
    "typescript": "2.0.2",
    "zone.js": "0.6.23"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^2.2.4",
    "clang-format": "^1.0.44",
    "codelyzer": "^0.0.28",
    "compression-webpack-plugin": "^0.3.1",
    "copy-webpack-plugin": "^3.0.0",
    "coveralls": "^2.11.13",
    "css-loader": "^0.25.0",
    "glob": "^7.0.3",
    "html-webpack-plugin": "^2.14.0",
    "istanbul-instrumenter-loader": "^1.0.0",
    "jasmine-core": "^2.5.2",
    "jasmine-matchers-loader": "^0.1.0",
    "json-loader": "^0.5.4",
    "karma": "^1.3.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.0.0",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-sauce-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "phantomjs-polyfill": "^0.0.2",
    "phantomjs-prebuilt": "^2.1.7",
    "protractor": "4.0.6",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "s3": "^4.4.0",
    "sauce-connect-launcher": "^0.16.0",
    "source-map-loader": "^0.1.5",
    "standard-version": "^2.2.1",
    "tslint": "^3.15.0",
    "webpack": "2.1.0-beta.25",
    "webpack-dev-server": "^2.1.0-beta.0",
    "webpack-md5-hash": "^0.0.5",
    "webpack-merge": "^0.14.0"
  }
}
